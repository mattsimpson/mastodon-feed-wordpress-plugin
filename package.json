{
  "name": "mastodon-feed",
  "version": "1.0.0",
  "description": "Display Mastodon feeds in WordPress via Gutenberg blocks and embeds",
  "main": "build/index.js",
  "scripts": {
    "build": "wp-scripts build src/block/index.js --output-path=build/block && npm run copy-block-json",
    "build:dist": "npm run build && node scripts/build-dist.js",
    "copy-block-json": "cp src/block/block.json build/block/block.json",
    "start": "wp-scripts start src/block/index.js --output-path=build/block",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "test": "wp-scripts test-unit-js --config=tests/js/jest.config.js",
    "test:watch": "wp-scripts test-unit-js --config=tests/js/jest.config.js --watch",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "wp-env": "wp-env",
    "wp-env:start": "wp-env start",
    "wp-env:stop": "wp-env stop",
    "wp-env:destroy": "wp-env destroy"
  },
  "keywords": [
    "mastodon",
    "wordpress",
    "gutenberg",
    "block",
    "feed",
    "social-media"
  ],
  "author": "Matt Simpson",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@wordpress/e2e-test-utils-playwright": "^1.32.0",
    "@wordpress/env": "^10.32.0",
    "@wordpress/scripts": "^30.26.0"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.32.0",
    "@wordpress/block-editor": "^15.5.0",
    "@wordpress/blocks": "^15.5.0",
    "@wordpress/components": "^30.5.0",
    "@wordpress/element": "^6.32.0",
    "@wordpress/i18n": "^6.5.0",
    "@wordpress/server-side-render": "^6.8.0"
  }
}
